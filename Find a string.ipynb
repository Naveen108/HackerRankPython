    "def count_substring(string, sub_string):\n",
    "    \n",
    "    lensub = len(sub_string)\n",
    "    count=0\n",
    "    i=0\n",
    "    #string[i:i+lensub]\n",
    "    while i<len(string):      \n",
    "        if(string[i:i+lensub]==sub_string):\n",
    "            count=count+1\n",
    "        i=i+1\n",
    "    return count"
   ]
  }
